<p id="notice"><%= notice %></p>

<h1>Channel Rates</h1>

<table>
  <thead>
    <tr>
      <th>Base rate type</th>
      <th>Modifier type</th>
      <th>Turned on</th>
      <th>Turned off on</th>
      <th>Rate</th>
      <th>Amount</th>
      <th>Channel name</th>
      <th>Channel guid</th>
      <th>Facility guid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @channel_rates.each do |channel_rate| %>
      <tr>
        <td><%= channel_rate.base_rate_type %></td>
        <td><%= channel_rate.modifier_type %></td>
        <td><%= channel_rate.turned_on %></td>
        <td><%= channel_rate.turned_off_on %></td>
        <td><%= channel_rate.rate %></td>
        <td><%= channel_rate.amount %></td>
        <td><%= channel_rate.channel_name %></td>
        <td><%= channel_rate.channel_guid %></td>
        <td><%= channel_rate.facility_guid %></td>
        <td><%= link_to 'Show', channel_rate %></td>
        <td><%= link_to 'Edit', edit_channel_rate_path(channel_rate) %></td>
        <td><%= link_to 'Destroy', channel_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Channel Rate', new_channel_rate_path %>
