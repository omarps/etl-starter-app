<p id="notice"><%= notice %></p>

<h1>Client Applications</h1>

<table>
  <thead>
    <tr>
      <th>Guid</th>
      <th>Name</th>
      <th>Channel rates on</th>
      <th>Internal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_applications.each do |client_application| %>
      <tr>
        <td><%= client_application.guid %></td>
        <td><%= client_application.name %></td>
        <td><%= client_application.channel_rates_on %></td>
        <td><%= client_application.internal %></td>
        <td><%= link_to 'Show', client_application %></td>
        <td><%= link_to 'Edit', edit_client_application_path(client_application) %></td>
        <td><%= link_to 'Destroy', client_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client Application', new_client_application_path %>
